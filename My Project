#include<iostream>
 using namespace std;

  int main(){

    string name = "Clark";
    char middle = "F.";
    string last = "Francisco";
    int age = 19;
    bool boolvalue = true;
    double height = 173;
    float weight = 91;
    
    cout << "Name: " << name << endl;
    cout << "MiddleName: " << middle << endl;
    cout << "LastName: " << last << endl;
    cout << "Age:" << age << endl;
    cout << "Male:" << boolvalue << endl;
    cout << "Height(cm): " << height << endl;
    cout << "Weight(kg): " << weight << endl;
    
    
    return 0;
}
