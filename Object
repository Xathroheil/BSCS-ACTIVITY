#include<iostream>
using namespace std;

class Person { 
      public:
          string name;
          string middle;
          string lastN;
          int age;
     };
 class Cat {
      public:
          string c1;
          string c2;  
     };
     
     int main() {
     
        Person person1;
        person1.name = "Clark";
        person1.middle = " F.";
        person1.lastN = " Francisco";
        person1.age = 19;
        
        Cat c1;
        c1.c1 = "Persian";
        c1.c2 = "White";
        
        cout << person1.name << person1.middle << person1.lastN << " is " << person1.age << " year old" << endl;
        cout << endl;
        cout << "My dream Cat is" << c1.c1 << " color" << c1.c2 << endl;
    return 0;
    }         
          
